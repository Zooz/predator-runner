{
  "name": "predator-runner",
  "version": "1.1.6",
  "description": "Docker image that runs predator tests",
  "main": "index.js",
  "scripts": {
    "system-tests": "./tests/run.sh system-tests",
    "local-system-tests": "./tests/run.sh local-system-tests",
    "integration-tests": "./tests/run.sh integration-tests",
    "unit-tests": "ARRIVAL_RATE=1 DURATION=1 nyc --check-coverage --lines 94 --reporter=html --reporter=text mocha ./tests/unit-tests --recursive",
    "lint": "node_modules/eslint/bin/eslint.js app/**",
    "release": "standard-version"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Zooz/predator-runner.git"
  },
  "keywords": [
    "runner",
    "performance",
    "artillery,",
    "predator"
  ],
  "author": "zooz",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Zooz/predator-runner/issues"
  },
  "homepage": "https://github.com/Zooz/predator-runner#readme",
  "dependencies": {
    "artillery": "https://github.com/Zooz/artillery.git#v2.0.2",
    "artillery-plugin-prometheus": "^1.1.0",
    "bunyan": "^1.8.12",
    "pidusage": "^2.0.17",
    "request": "~2.87.0",
    "requestxn": "^3.1.4",
    "uuid": "^3.3.3"
  },
  "devDependencies": {
    "chai": "^4.1.2",
    "colors": "^1.3.3",
    "eslint": "^6.2.2",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-node": "^9.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "hapi": "^17.8.1",
    "lodash": "^4.17.15",
    "mocha": "^5.1.0",
    "mochawesome": "^3.0.2",
    "nock": "^10.0.6",
    "nyc": "^14.1.1",
    "proxyquire": "^2.0.1",
    "request-promise-native": "^1.0.5",
    "rewire": "^4.0.1",
    "should": "^13.2.1",
    "sinon": "4.1.3",
    "standard-version": "^6.0.1"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  }
}
