{
  "name": "predator-runner",
  "version": "1.3.0",
  "description": "Docker image that runs predator tests",
  "main": "index.js",
  "scripts": {
    "system-tests": "./tests/run.sh system-tests",
    "local-system-tests": "./tests/run.sh local-system-tests",
    "integration-tests": "./tests/run.sh integration-tests",
    "unit-tests": "ARRIVAL_RATE=1 DURATION=1 nyc --check-coverage --lines 95 --reporter=html --reporter=text mocha ./tests/unit-tests --recursive --exit",
    "lint": "node_modules/eslint/bin/eslint.js app/**",
    "release": "standard-version"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Zooz/predator-runner.git"
  },
  "keywords": [
    "runner",
    "performance",
    "artillery,",
    "predator"
  ],
  "author": "zooz",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Zooz/predator-runner/issues"
  },
  "homepage": "https://github.com/Zooz/predator-runner#readme",
  "dependencies": {
    "artillery": "https://github.com/Zooz/artillery.git#v2.0.2",
    "artillery-plugin-prometheus": "^1.1.0",
    "bunyan": "^1.8.14",
    "csv-parse": "^4.8.8",
    "pidusage": "^2.0.20",
    "request": "~2.87.0",
    "requestxn": "^3.1.4",
    "uuid": "^3.3.3"
  },
  "devDependencies": {
    "@hapi/hapi": "^18.4.1",
    "chai": "^4.1.2",
    "colors": "^1.3.3",
    "eslint": "^6.2.2",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-node": "^9.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "lodash": "^4.17.19",
    "mocha": "^8.0.1",
    "mochawesome": "^4.1.0",
    "nock": "^10.0.6",
    "nyc": "^14.1.1",
    "proxyquire": "^2.1.3",
    "request-promise-native": "^1.0.8",
    "rewire": "^5.0.0",
    "should": "^13.2.3",
    "sinon": "4.1.3",
    "standard-version": "^8.0.2"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  }
}
